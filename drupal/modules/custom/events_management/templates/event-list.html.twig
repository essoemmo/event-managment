{% if events %}
    <h2>Events</h2>
    <ul>
        {% for event in events %}
            <li>
                <h3>{{ event.title }}</h3>
                <p><strong>Category:</strong> {{ event.category }}</p>
                <p><strong>Start Time:</strong> {{ event.start_time|date("Y-m-d H:i") }}</p>
                <p><strong>End Time:</strong> {{ event.end_time|date("Y-m-d H:i") }}</p>
                <p>{{ event.description }}</p>
            </li>
        {% endfor %}
    </ul>
{% else %}
    <p>No events found.</p>
{% endif %}
